{
  "name": "dhanashree--backend",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "start": "node build/src/server.js",
    "build": "tsc",
    "start:dev": "ts-node-dev ./src/server.ts",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "seed:admin": "ts-node-dev  ./src/seeder/admin.seed.ts seed",
    "seed:province": "ts-node-dev src/seeder/address.seed.ts -province",
    "seed:district": "ts-node-dev src/seeder/address.seed.ts -district",
    "seed:municipality": "ts-node-dev src/seeder/address.seed.ts -municipality",
    "seed:ward": "ts-node-dev src/seeder/address.seed.ts -ward",
    "seed": "npm run seed:admin && npm run seed:province && npm run seed:district && npm run seed:municipality && npm run seed:ward",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -d ./src/config/database.config.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/config/database.config.ts",
    "migration:up": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/config/database.config.ts",
    "migration:down": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/config/database.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.6.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "file-type": "^16.5.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^4.0.6",
    "nodemailer": "^7.0.12",
    "pg": "^8.17.2",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.2.2",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.9.1",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.1",
    "@types/nodemailer": "^6.4.17",
    "@types/readline-sync": "^1.4.8",
    "@types/streamifier": "^0.1.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
